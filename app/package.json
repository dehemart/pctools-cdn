{
  "name": "pctools-images",
  "version": "0.0.1",
  "main": "src/index.js",
  "author": "dehema",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/index.ts",
    "prebuild": "yarn install && rm -fr dist src/@types",
    "build": "tsc --noEmit false --project tsconfig.json",
    "start:prd": "node ./dist/index.js",
    "postbuild": "tscpaths -p tsconfig.json -s ./src -o ./dist",
    "docker:up": "docker compose up -d --wait",
    "docker:down": "docker compose down",
    "docker:remove_volume": "docker volume rm pctools-back_postgres",
    "migrate": "yarn prisma migrate dev",
    "test": " yarn prisma migrate dev --name test && jest -i"
  },
  "prisma": {
    "seed": "ts-node-dev ./prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "audit": "^0.0.6",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-winston": "^4.2.0",
    "fix": "^0.0.6",
    "i18next": "^23.5.1",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "npm": "^10.8.2",
    "prisma": "^5.17.0",
    "sharp": "^0.33.5",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.2",
    "zod-i18n-map": "^2.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "@jest/globals": "^29.7.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.6.0",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.49.0",
    "http-status-codes": "^2.2.0",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": "20.x"
  }
}
